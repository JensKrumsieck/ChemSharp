;$COMMENT=
# 170 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/Avance.incl"
;$Id: Avance3aqs.incl,v 1.1.8.3 2015/03/05 16:46:54 ber Exp $
# 14 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30" 2


# 17 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30"
"acqt0=-p1*0.66/3.1416"


# 1 "mc_line 20 file C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30 exp. def. part of mc cmd. before ze"
; dimension 1D; AQ_mode 
define delay MCWRK
define delay MCREST
"MCREST = 30m - 30m"
"MCWRK = 0.333333*30m"

    dccorr
# 20 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30"
1 ze
# 1 "mc_line 20 file C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30 exp. def. of mc cmd. after ze"
# 21 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30"
# 1 "mc_line 21 file C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30 exp. start label for mc cmd."
2 MCWRK  * 2
LBLF0, MCWRK
  MCREST
# 22 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30"
  d1
  p1*0.33 ph1
  go=2 ph31
# 1 "mc_line 25 file C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30 exp. mc cmd. in line"
  MCWRK wr #0
  MCWRK zd
  lo to LBLF0 times td0

# 26 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30"
exit
# 29 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30"
ph1=0 2 2 0 1 3 3 1
ph31=0 2 2 0 1 3 3 1


;pl1 : f1 channel - power level for pulse (default)
;p1 : f1 channel -  90 degree high power pulse
;d1 : relaxation delay; 1-5 * T1
;ns: 1 * n, total number of scans: NS * TD0
# 40 "C:/Bruker/TopSpin3.5pl5/exp/stan/nmr/lists/pp/zg30"
